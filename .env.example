# Core AI (Required)
# Single key (backward compatibility):
GOOGLE_API_KEY=xxx
GEMINI_API_KEY=xxx

# Multiple keys for fallback (recommended for production):
GOOGLE_API_KEY_1=your_first_google_key
GOOGLE_API_KEY_2=your_second_google_key
GOOGLE_API_KEY_3=your_third_google_key

# Database & Auth
DB_BACKEND=supabase          # or "local" for development
AUTH_BACKEND=supabase        # or "local" for development  
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_ANON_KEY=xxx

# Email
RESEND_API_KEY=re_xxx
EMAIL_FROM=Amble <onboarding@yourdomain.com>

# Push Notifications
VAPID_PRIVATE_KEY=xxx
VAPID_PUBLIC_KEY=xxx
VAPID_CONTACT_EMAIL=admin@yourdomain.com

# Chat & Video Calls
COMETCHAT_APP_ID=xxx
COMETCHAT_AUTH_KEY=xxx
COMETCHAT_REGION=us

# Long-term Memory (Optional)
MEM0_API_KEY=xxx

# ElevenLabs Conversational AI (Optional - for voice avatar)
# Get your Agent ID from: https://elevenlabs.io/conversational-ai
# Free tier: ~15 mins/month of conversation
VITE_ELEVENLABS_AGENT_ID=
# Optional: API key for private agents
ELEVENLABS_API_KEY=

# Anam AI Visual Avatar (Optional - for lip-sync video avatar)
# Get your API key from: https://www.anam.ai/
# Single key (backward compatibility):
VITE_ANAM_API_KEY=

# Multiple keys for fallback (recommended for production):
ANAM_API_KEY_1=your_first_anam_key
ANAM_API_KEY_2=your_second_anam_key
ANAM_API_KEY_3=your_third_anam_key

# Environment
ENV=production