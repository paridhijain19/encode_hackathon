# ===========================================
# Amble Environment Configuration Template
# ===========================================
# Copy this file to .env and fill in your actual values.
# DO NOT commit .env with real secrets to version control.

# ------------------------------------------
# Google Gemini / ADK Configuration
# ------------------------------------------
GOOGLE_GENAI_USE_VERTEXAI=FALSE
GOOGLE_API_KEY=your_google_api_key_here

# ------------------------------------------
# Mem0 Long-Term Memory
# ------------------------------------------
# Get your API key from: https://app.mem0.ai/
MEM0_API_KEY=your_mem0_api_key_here

# Optional: Scope memory under organization/project
# MEM0_ORG_ID=your_org_id
# MEM0_PROJECT_ID=your_project_id

# ------------------------------------------
# Opik Observability (Comet)
# ------------------------------------------
# Get your API key from: https://www.comet.com/
OPIK_API_KEY=your_opik_api_key_here

# ------------------------------------------
# Optional Development Settings
# ------------------------------------------
# DEBUG=true
# LOG_LEVEL=INFO
